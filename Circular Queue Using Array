#include <stdio.h>
#define MAX 100
int cq[MAX], front = -1, rear = -1, n, val;
void insert() {
    if ((front == 0 && rear == MAX-1) || (front == rear+1)) printf("Overflow\n");
    else {
        printf("Insert value: "); scanf("%d", &val);
        if (front == -1) { front = 0; rear = 0; }
        else if (rear == MAX-1) rear = 0;
        else rear++;
        cq[rear] = val;
    }
}
void delete() {
    if (front == -1) printf("Underflow\n");
    else {
        printf("Deleted: %d\n", cq[front]);
        if (front == rear) front = rear = -1;
        else if (front == MAX-1) front = 0;
        else front++;
    }
}
void display() {
    int i;
    if (front == -1) printf("Empty\n");
    else {
        i = front;
        while (1) {
            printf("%d ", cq[i]);
            if (i == rear) break;
            if (i == MAX-1) i = 0; else i++;
        }
        printf("\n");
    }
}
int main() {
    int ch;
    while (1) {
        printf("1.Insert 2.Delete 3.Display 4.Exit\n");
        scanf("%d", &ch);
        if (ch == 1) insert();
        else if (ch == 2) delete();
        else if (ch == 3) display();
        else break;
    }
    return 0;
}
