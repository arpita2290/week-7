#include <stdio.h>
#define MAX 100
int queue[MAX], front = -1, rear = -1, n, i, val;
void insert() {
    if (rear == MAX - 1) printf("Overflow\n");
    else {
        if (front == -1) front = 0;
        printf("Insert value: ");
        scanf("%d", &val);
        rear++;
        queue[rear] = val;
    }
}
void delete() {
    if (front == -1 || front > rear) printf("Underflow\n");
    else {
        printf("Deleted: %d\n", queue[front]);
        front++;
    }
}
void display() {
    if (front == -1 || front > rear) printf("Empty\n");
    else {
        for (i = front; i <= rear; i++) printf("%d ", queue[i]);
        printf("\n");
    }
}
int main() {
    int ch;
    while (1) {
        printf("1.Insert 2.Delete 3.Display 4.Exit\n");
        scanf("%d", &ch);
        if (ch == 1) insert();
        else if (ch == 2) delete();
        else if (ch == 3) display();
        else break;
    }
    return 0;
}
