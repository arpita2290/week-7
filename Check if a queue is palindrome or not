#include <stdio.h>
#define MAX 100
int queue[MAX], front = 0, rear = -1;
void enqueue(int val) {
    if (rear == MAX - 1) return;
    queue[++rear] = val;
}
int isPalindrome() {
    int i = front, j = rear;
    while (i < j) {
        if (queue[i] != queue[j]) return 0;
        i++; j--;
    }
    return 1;
}
int main() {
    int n, val;
    printf("Number of elements: ");
    scanf("%d", &n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &val);
        enqueue(val);
    }
    if (isPalindrome()) printf("Palindrome\n");
    else printf("Not palindrome\n");
    return 0;
}
